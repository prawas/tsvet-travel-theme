u043e\u0436\u0435\u043d \u0432\u043e\u0437\u043b\u0435 \u043f\u043b\u044f\u0436\u0430\r\n\u0432 \u0446\u0435\u043d\u0442\u0440\u0435 \u0441\u0442\u043e\u043b\u0438\u0446\u044b \u043e\u0441\u0442\u0440\u043e\u0432\u0430 \u0410\u0440\u0440\u0435\u0441\u0438\u0444\u0435\r\n\u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u044b\u0439 \u0432\u0438\u0434 \u043d\u0430 \u043e\u043a\u0435\u0430\u043d\r\n\u0432\u044b\u0441\u043e\u043a\u0438\u0439 \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u043c\u043e\u0433\u043e \u043e\u0431\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u043d\u0438\u044f\r\n\u0443\u0434\u043e\u0431\u0441\u0442\u0432\u0430 \u0434\u043b\u044f \u0438\u043d\u0432\u0430\u043b\u0438\u0434\u043e\u0432","post_content":"<div class=\"product-productdescription\"> <p><strong data-block-type=\"raspolozhenie\">\u0420\u0410\u0421\u041f\u041e\u041b\u041e\u0416\u0415\u041d\u0418\u0415: <\/strong> \u0432 \u0432\u043e\u0441\u0442\u043e\u0447\u043d\u043e\u0439 \u0447\u0430\u0441\u0442\u0438 \u043e\u0441\u0442\u0440\u043e\u0432\u0430, \u0432 \u0446\u0435\u043d\u0442\u0440\u0435 \u0441\u0442\u043e\u043b\u0438\u0446\u044b \u043e\u0441\u0442\u0440\u043e\u0432\u0430 - \u0410\u0420\u0420\u0415\u0421\u0418\u0424\u0415 \u0441 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430\u043c\u0438, \u0431\u0430\u0440\u0430\u043c\u0438 \u0438 \u0440\u0435\u0441\u0442\u043e\u0440\u0430\u043d\u0430\u043c\u0438; \u043e\u043a. 7 \u043a\u043c \u043e\u0442 \u0430\u044d\u0440\u043e\u043f\u043e\u0440\u0442\u0430 \u0432 \u0410\u0440\u0440\u0435\u0441\u0438\u0444\u0435; \u0432\u0431\u043b\u0438\u0437\u0438 \u043e\u0442\u0435\u043b\u044f \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u0430\u044f \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0430, \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0430 \u0442\u0430\u043a\u0441\u0438.<\/p><p><strong data-block-type=\"plyazh\">\u041f\u041b\u042f\u0416: <\/strong> El Reducto, \u043e\u0431\u0449\u0435\u0434\u043e\u0441\u0442\u0443\u043f\u043d\u044b\u0439, \u043f\u0435\u0441\u0447\u0430\u043d\u044b\u0439, \u0448\u0438\u0440\u043e\u043a\u0438\u0439, \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0430\u043c\u044b\u0445 \u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u043d\u0430 \u043e\u0441\u0442\u0440\u043e\u0432\u0435 \u0441\u043e \u0441\u0432\u0435\u0442\u043b\u044b\u043c \u043f\u0435\u0441\u043a\u043e\u043c, \u0432\u043e\u0437\u043b\u0435 \u043e\u0442\u0435\u043b\u044f; \u043d\u0430 \u043f\u043b\u044f\u0436\u0435 \u0434\u0443\u0448\u0438.<\/p><p><strong data-block-type=\"otel\">\u041e\u0422\u0415\u041b\u042c: <\/strong> \u043f\u044f\u0442\u0438\u0437\u0432\u0435\u0437\u0434\u043e\u0447\u043d\u044b\u0439, \u0433\u043e\u0440\u043e\u0434\u0441\u043a\u043e\u0439, \u044d\u043b\u0435\u0433\u0430\u043d\u0442\u043d\u044b\u0439 \u0438 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0439, \u043e\u0431\u043d\u043e\u0432\u043b\u0435\u043d \u0432 2004 \u0433., \u0432\u0441\u0435\u0433\u043e 160 \u043d\u043e\u043c\u0435\u0440\u043e\u0432, 18 \u044d\u0442\u0430\u0436\u0435\u0439, 4 \u043b\u0438\u0444\u0442\u0430, \u044d\u043b\u0435\u0433\u0430\u043d\u0442\u043d\u043e\u0435 \u043b\u043e\u0431\u0431\u0438, \u043a\u0440\u0443\u0433\u043b\u043e\u0441\u0443\u0442\u043e\u0447\u043d\u044b\u0439 \u0440\u0435\u0441\u0435\u043f\u0448\u0435\u043d, 2 \u0440\u0435\u0441\u0442\u043e\u0440\u0430\u043d\u0430 - \u0441\u0440\u0435\u0434\u0438\u0437\u0435\u043c\u043d\u043e\u043c\u043e\u0440\u0441\u043a\u0430\u044f \u0438 \u0438\u043d\u0442\u0435\u0440\u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u043a\u0443\u0445\u043d\u0438, \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0434\u0440\u0435\u0441\u0441-\u043a\u043e\u0434 - \u043c\u0443\u0436\u0447\u0438\u043d\u044b \u0434\u043b\u0438\u043d\u043d\u044b\u0435 \u0431\u0440\u044e\u043a\u0438, \u0431\u0430\u0440 \u0443 \u0431\u0430\u0441\u0441\u0435\u0439\u043d\u0430, \u0447\u0438\u0442\u0430\u043b\u044c\u043d\u044b\u0439 \u0437\u0430\u043b; \u043a\u043e\u043d\u0444\u0435\u0440\u0435\u043d\u0446-\u0437\u0430\u043b, \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0439 \u0431\u0435\u0441\u043f\u0440\u043e\u0432\u043e\u0434\u043d\u043e\u0439 \u0418\u043d\u0442\u0435\u0440\u043d\u0435\u0442; \u0437\u0430 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443: \u0432\u0445\u043e\u0434 \u0432 \u0418\u043d\u0442\u0435\u0440\u043d\u0435\u0442, \u043f\u0440\u0430\u0447\u0435\u0447\u043d\u0430\u044f, \u043f\u0430\u0440\u0438\u043a\u043c\u0430\u0445\u0435\u0440\u0441\u043a\u0430\u044f, \u043f\u0440\u043e\u043a\u0430\u0442 \u0430\u0432\u0442\u043e\u043c\u043e\u0431\u0438\u043b\u0435\u0439 \u0438 \u0432\u0435\u043b\u043e\u0441\u0438\u043f\u0435\u0434\u043e\u0432; \u0443\u0434\u043e\u0431\u0441\u0442\u0432\u0430 \u0434\u043b\u044f \u0438\u043d\u0432\u0430\u043b\u0438\u0434\u043e\u0432; \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u043a\u0440\u0435\u0434\u0438\u0442\u043d\u044b\u0435 \u043a\u0430\u0440\u0442\u043e\u0447\u043a\u0438: Visa, MasterCard, American Express, Diners Club.<\/p><p><strong data-block-type=\"nomer\">\u041d\u041e\u041c\u0415\u0420:<\/strong><strong>\u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u044b\u0439: <\/strong>2-\u043c\u0435\u0441\u0442., o\u043a. 30 \u043c\u00b2, \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0439 \u0438 \u044d\u043b\u0435\u0433\u0430\u043d\u0442\u043d\u044b\u0439, \u0438\u043d\u0434\u0438\u0432\u0438\u0434\u0443\u0430\u043b\u044c\u043d\u043e \u0443\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u043c\u044b\u0439 \u043a\u043e\u043d\u0434\u0438\u0446\u0438\u043e\u043d\u0435\u0440, \u0432\u0430\u043d\u043d\u0430\u044f \u043a\u043e\u043c\u043d\u0430\u0442\u0430 (\u0434\u0443\u0448, \u0442\u0443\u0430\u043b\u0435\u0442; \u0444\u0435\u043d, \u0445\u0430\u043b\u0430\u0442), \u0441\u043f\u0443\u0442\u043d\u0438\u043a\u043e\u0432\u043e\u0435 \u0442\u0435\u043b\u0435\u0432\u0438\u0434\u0435\u043d\u0438\u0435, \u0442\u0435\u043b\u0435\u0444\u043e\u043d, \u0441\u0435\u0439\u0444; \u0437\u0430 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443: \u043c\u0438\u043d\u0438-\u0431\u0430\u0440; \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e \u043a\u0440\u043e\u0432\u0430\u0442\u043a\u0430 \u0434\u043b\u044f \u0440\u0435\u0431\u0435\u043d\u043a\u0430 \u0434\u043e 2 \u043b\u0435\u0442 \u043f\u043e \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u0437\u0430\u043a\u0430\u0437\u0443 \u0434\u043e \u043f\u0440\u0438\u0435\u0437\u0434\u0430; \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u0435\u043c\u044b\u0435 \u043e\u043a\u043d\u0430, \u043d\u0430 \u043e\u043a\u043d\u0430\u0445 \u0448\u0442\u043e\u0440\u044b\/\u0441\u0432\u0435\u0442\u043e\u043d\u0435\u043f\u0440\u043e\u043d\u0438\u0446\u0430\u0435\u043c\u044b\u0435 \u0437\u0430\u043d\u0430\u0432\u0435\u0441\u043a\u0438; \u0432\u0438\u0434 \u043d\u0430 \u043c\u043e\u0440\u0435 \u0437\u0430 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443; <strong>suite \u0441 \u0432\u0438\u0434\u043e\u043c \u043d\u0430 \u043c\u043e\u0440\u0435: <\/strong>2-\u043c\u0435\u0441\u0442. (\u043c\u043e\u0436\u043d\u043e \u0440\u0430\u0437\u043c\u0435\u0441\u0442\u0438\u0442\u044c \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u043e 1 \u043a\u0440\u043e\u0432\u0430\u0442\u044c), o\u043a. 45 \u043c\u00b2, \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0439 \u0438 \u044d\u043b\u0435\u0433\u0430\u043d\u0442\u043d\u044b\u0439, \u0438\u043d\u0434\u0438\u0432\u0438\u0434\u0443\u0430\u043b\u044c\u043d\u043e \u0443\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u043c\u044b\u0439 \u043a\u043e\u043d\u0434\u0438\u0446\u0438\u043e\u043d\u0435\u0440, \u0432\u0430\u043d\u043d\u0430\u044f \u043a\u043e\u043c\u043d\u0430\u0442\u0430 (\u0434\u0443\u0448, \u0442\u0443\u0430\u043b\u0435\u0442; \u0444\u0435\u043d, \u0445\u0430\u043b\u0430\u0442), \u0434\u0432\u0430 \u043f\u043e\u043c\u0435\u0449\u0435\u043d\u0438\u044f, \u0441\u043f\u0430\u043b\u044c\u043d\u044f \u0440\u0430\u0437\u0434\u0435\u043b\u0435\u043d\u0430 \u0441 \u0433\u043e\u0441\u0442\u0438\u043d\u043e\u0439 \u043f\u0435\u0440\u0435\u0434\u0432\u0438\u0436\u043d\u043e\u0439 \u0434\u0432\u0435\u0440\u044c\u044e, \u0441\u043f\u0443\u0442\u043d\u0438\u043a\u043e\u0432\u043e\u0435 \u0442\u0435\u043b\u0435\u0432\u0438\u0434\u0435\u043d\u0438\u0435, \u0442\u0435\u043b\u0435\u0444\u043e\u043d, \u0441\u0435\u0439\u0444; \u0437\u0430 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443: \u043c\u0438\u043d\u0438-\u0431\u0430\u0440; \u0431\u0435\u0441\u043f\u043b\u0430\u044c\u043d\u043e \u043a\u0440\u043e\u0432\u0430\u0442\u043a\u0430 \u0434\u043b\u044f \u0440\u0435\u0431\u0435\u043d\u043a\u0430 \u0434\u043e 2 \u043b\u0435\u0442 \u043f\u043e \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u0437\u0430\u043a\u0430\u0437\u0443 \u0434\u043e \u043f\u0440\u0438\u0435\u0437\u0434\u0430; \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u0435\u043c\u044b\u0435 \u043e\u043a\u043d\u0430, \u043d\u0430 \u043e\u043a\u043d\u0430\u0445 \u0441\u0432\u0435\u0442\u043e\u043d\u0435\u043f\u0440\u043e\u043d\u0438\u0446\u0430\u0435\u043c\u044b\u0435 \u0448\u0442\u043e\u0440\u044b\/\u0437\u0430\u043d\u0430\u0432\u0435\u0441\u043a\u0438; \u0432\u0438\u0434 \u043d\u0430 \u043c\u043e\u0440\u0435.<\/p><p><strong data-block-type=\"sport-i-razvlecheniya\">\u0421\u041f\u041e\u0420\u0422 \u0418 \u0420\u0410\u0417\u0412\u041b\u0415\u0427\u0415\u041d\u0418\u042f: <\/strong> \u043a\u0440\u044b\u0442\u044b\u0439 \u0431\u0430\u0441\u0441\u0435\u0439\u043d, \u0433\u043b. o\u043a. 1,2 \u043c, \u043f\u0440\u0435\u0441\u043d\u0430\u044f \u0432\u043e\u0434\u0430, \u0443 \u0431\u0430\u0441\